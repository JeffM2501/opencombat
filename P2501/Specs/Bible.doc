\par {\listtext\pard\plain\s2 \b\f36\fs28\insrsid12343095 \hich\af36\dbch\af0\loch\f36 2.1\tab}}\pard\plain \s2\ql \li0\ri0\sb280\sa280\sl240\slmult0
